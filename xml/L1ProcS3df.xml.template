      <variables>
        <var name="%(digi_versionName)s">%(digi_version)s</var>
        <var name="%(verifyLog_versionName)s">%(verifyLog_version)s</var>
        <var name="%(verifyHisto_versionName)s">%(verifyHisto_version)s</var>
