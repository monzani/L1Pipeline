    </process>

    <process name="registerOrbitPlot" site="${jobsite}" autoRetryMaxAttempts="%(retries)s">
      <variables>
        <var name="parentProcess">drawOrbit</var>
        <var name="fileType">orbitPlot</var>
        <var name="timeProcess">findChunks</var>
      </variables>
      <script><![CDATA[
%(registerBody)s
]]>
      </script>
      <depends>
